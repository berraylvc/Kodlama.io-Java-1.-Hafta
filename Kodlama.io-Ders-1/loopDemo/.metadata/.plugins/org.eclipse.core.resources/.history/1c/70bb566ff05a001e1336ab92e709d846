package miniProjectSayiBulma;

public class Main {
	public static void main(String[] args) {
		
		int [] sayilar = new int [] {1,2,5,7,9,0};
		int aranacak = 5;
		
		for (int i=1;i<sayilar.length;i++) {
			if (aranacak ==i) {
				System.out.println("Sayi Mevcuttur.");
			}else {
				System.out.println("Sayi Mevcut Degildir.");
			}
			
		}
	}
	
}
	